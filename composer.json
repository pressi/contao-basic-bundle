{
    "name"          : "2do/contao-basic-bundle",
    "type"          : "contao-bundle",
    "description"   : "Contao 4 IIDO Basic Bundle",
    "homepage"      : "http://www.iido.at",
    "license"       : "MIT",
    "authors"       :
    [
        {
            "name"      : "Stephan PreÃŸl",
            "email"     : "development@prestep.at",
            "homepage"  : "http://www.prestep.at",
            "role"      : "Developer"
        }
    ],
    "require":
    {
        "php": ">=7.1",
        "contao/core-bundle": "~4",
        "contao/news-bundle": "~4",
        "contao/calendar-bundle": "~4",
        "madeyourday/contao-rocksolid-custom-elements": "~2",
        "codefog/contao-cookiebar": "~2",
        "menatwork/contao-multicolumnwizard-bundle": "~3"
    },
    "require-dev":
    {
        "contao/test-case": "1.1.*",
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpunit/phpunit": "^6.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "php-coveralls/php-coveralls": "^2.0",
        "symfony/phpunit-bridge": "^3.2"
    },
    "conflict":
    {
        "contao/core": "*"
    },
    "autoload":
    {
        "psr-4":
        {
            "IIDO\\BasicBundle\\": "src/"
        },

        "exclude-from-classmap":
        [
            "tests/",
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "IIDO\\BasicBundle\\Tests\\": "tests/"
        }
    },
    "config":
    {
        "preferred-install"         : "dist"
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-develop" : "4.4.x-dev"
        },
        "contao-manager-plugin" : "IIDO\\BasicBundle\\ContaoManager\\Plugin"
    }
}