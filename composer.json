{
    "name"          : "2do/contao-basic-bundle",
    "type"          : "contao-bundle",
    "description"   : "Contao 4 IIDO Basic Bundle",
    "homepage"      : "http://www.iido.at",
    "license"       : "MIT",
    "authors"       :
    [
        {
            "name"      : "Stephan PreÃŸl",
            "email"     : "development@prestep.at",
            "homepage"  : "http://www.prestep.at",
            "role"      : "Developer"
        }
    ],
    "require":
    {
        "php": ">=7.0",
        "contao/core-bundle": "~4",
        "contao/news-bundle": "~4",
        "contao/calendar-bundle": "~4",
        "madeyourday/contao-rocksolid-custom-elements": "~2",
        "codefog/contao-cookiebar": "~2",
        "menatwork/contao-multicolumnwizard": "~3"
    },
    "conflict":
    {
        "contao/core": "*"
    },
    "autoload":
    {
        "psr-4":
        {
            "IIDO\\BasicBundle\\": "src/"
        },

        "exclude-from-classmap":
        [
            "tests/",
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "config":
    {
        "preferred-install"         : "dist"
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-develop" : "4.4.x-dev"
        },
        "contao-manager-plugin" : "IIDO\\BasicBundle\\ContaoManager\\Plugin"
    }
}