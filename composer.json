{
    "name"          : "2do/contao-basic-bundle",
    "type"          : "contao-bundle",
    "description"   : "Contao 4 IIDO Basic Bundle",
    "homepage"      : "http://www.iido.at",
    "license"       : "MIT",
    "authors"       :
    [
        {
            "name"      : "Stephan PreÃŸl",
            "email"     : "development@prestep.at",
            "homepage"  : "http://www.prestep.at",
            "role"      : "Developer"
        }
    ],
    "version": "0.5.0",
    "require":
    {
        "php": ">=7.3",
        "contao/core-bundle": ">=4.9.*",
        "symfony/framework-bundle": ">=4.9.*",
        "madeyourday/contao-rocksolid-custom-elements": ">=2.*",
        "heimrichhannot/contao-fieldpalette-bundle": "^0.*"
    },
    "replace":
    {
        "2do/contao-config-bundle": "*"
    },
    "conflict":
    {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "suggest":
    {
        "2do/contao-master-connect-bundle": "IIDO Master Connection Bundle",
        "2do/contao-projects-bundle": "IIDO Projects Bundle",
        "2do/contao-hotel-bundle": "IIDO Hotel Bundle"
    },
    "autoload":
    {
        "psr-4":
        {
            "IIDO\\BasicBundle\\": "src/"
        }
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra":
    {
        "contao-manager-plugin" : "IIDO\\BasicBundle\\ContaoManager\\Plugin"
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "IIDO\\BasicBundle\\Tests\\": "tests/"
        }
    },
    "support":
    {
        "issues": "https://github.com/pressi/contao-basic-bundle/issues",
        "source": "https://github.com/pressi/contao-basic-bundle"
    }
}